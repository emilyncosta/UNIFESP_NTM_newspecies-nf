workDir = 's3://YOUR WORK DIR' // <- replace with your own bucket!

process {
        errorStrategy = 'retry'
        executor = 'awsbatch'
        }

aws {
        accessKey = '<YOUR S3 ACCESS KEY>'
        secretKey = '<YOUR S3 SECRET KEY>'
        region = 'YOUR REGION'
        client.uploadMaxThreads = 4
        }
